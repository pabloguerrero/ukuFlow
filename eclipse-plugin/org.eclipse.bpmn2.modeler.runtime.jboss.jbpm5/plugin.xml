							<property name="name" value="Comment"/>
						</value>
					</property>
					<property name="dataInputs">
						<value>
							<property name="name" value="GroupId"/>
						</value>
					</property>
					<property name="dataInputs">
						<value>
							<property name="name" value="Skippable"/>
